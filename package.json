{
  "name": "concordia-course-to-transcript",
  "version": "1.0.0",
  "description": "This project aims to download, sort, and transcribe course content (audio and video files) from the Concordia University's Renewable Energy course. The course content is downloaded from the eConcordia website, sorted into appropriate directories, and transcribed using Google Cloud Speech-to-Text API.",
  "sourceType": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mueedqadri/concordia-course-to-transcript.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Mueed Qadri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mueedqadri/concordia-course-to-transcript/issues"
  },
  "homepage": "https://github.com/mueedqadri/concordia-course-to-transcript",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "eslint": "^8.37.0",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@google-cloud/speech": "^5.4.0",
    "@google-cloud/storage": "^6.9.5",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2"
  }
}
